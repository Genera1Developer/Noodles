{
  "version": 2,
  "builds": [
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "functions": {
    "public/main/hello.js": { "memory": 128, "maxDuration": 5 }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/main/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}