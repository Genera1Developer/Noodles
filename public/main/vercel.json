{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node@latest"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "api/hello.js": { "memory": 128, "maxDuration": 5 }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/main/$1" }
  ]
}